clear
con = m_constant;

Di(3,10).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/init216/anneal/','Di_b75.gout');
VOLUME = (3*5.43E-10)^3;
cond(3,10) = (con.kb / VOLUME)*sum(Di(3,10).Di(:,3));

Di(5,10).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perfect2/anneal/b75/','Di.gout');
VOLUME = (5*5.43E-10)^3;
cond(5,10) = (con.kb / VOLUME)*sum(Di(5,10).Di(:,3));

Di(8,10).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perf4096/anneal_1100K/emin/','Di_b75.gout');
VOLUME = (8*5.43E-10)^3;
cond(8,10) = (con.kb / VOLUME)*sum(Di(8,10).Di(:,3));




loglog(...
    Di(3,10).Di(:,2),Di(3,10).Di(:,3),...
    Di(5,10).Di(:,2),Di(5,10).Di(:,3),...
    Di(8,10).Di(:,2),Di(8,10).Di(:,3),...
    linspace(min(Di(5,10).Di(:,2)),max(Di(5,10).Di(:,2)),100),...
    1E21*linspace(min(Di(5,10).Di(:,2)),max(Di(5,10).Di(:,2)),100).^(-2),...
    linspace(min(Di(5,10).Di(:,2)),max(Di(5,10).Di(:,2)),100),...
    1E12*2*pi./linspace(min(Di(5,10).Di(:,2)),max(Di(5,10).Di(:,2)),100)...
    )

