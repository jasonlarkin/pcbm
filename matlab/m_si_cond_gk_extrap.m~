clear
constant = m_constant;
size_big = 100000;

gk = m_si_cond_alloy_gk_results

plot(...
    1./[gk.size;size_big] , 1./[gk.cond(:,2);8] , '.',...
    1./[gk.size;size_big] , 1./[gk.cond(:,4);3.4] , '.'...
    )

[p R] = m_cond_extrap(...
    [gk.size( size(gk.size,1) - 3 : size(gk.size,1))  ],...
    [gk.cond( size(gk.size,1) - 3 : size(gk.size,1),2)]...
    );
R
1./p(2)
[p R] = m_cond_extrap(...
    [gk.size( size(gk.size,1) - 3 : size(gk.size,1))  ],...
    [gk.cond( size(gk.size,1) - 3 : size(gk.size,1),4)]...
    );
R
1./p(2)
