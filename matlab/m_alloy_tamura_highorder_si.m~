clear

constant = m_cons

%si
nmd =...
    load(['/home/jason/disorder2/si/alloy/0.5/20x/' 'nmd.mat']);
% sed =...
%     load(['/home/jason/disorder2/si/alloy/0.5/20x/' 'SEDdata.mat']);
alloy =...
    load(['/home/jason/disorder2/si/alloy/0.5/20x/' 'ALLOY.mat']);
m1 = 1; m2=3; c = 0.45; vm = (1-c)*m1 + c*m2;
g(1) =...
    (1-c)* ((1 - (m1/vm) )^1) + c* ((1 - (m2/vm) )^1);
g(2) =...
    (1-c)* ((1 - (m1/vm) )^2) + c* ((1 - (m2/vm) )^2);
g(3) =...
    (1-c)* ((1 - (m1/vm) )^3) + c* ((1 - (m2/vm) )^3);
g(4) =...
    (1-c)* ((1 - (m1/vm) )^4) + c* ((1 - (m2/vm) )^4);
tamura(1).tamura = m_alloy_tamura_highorder_Iw(g,nmd.freq,nmd.NUM_UCELL_COPIES,10000,100);

%--------------------------------------------------------------------------
pause
%--------------------------------------------------------------------------
subplot(3,1,1),...
semilogx(...
    tamura(1).tamura.freq_range,tamura(1).tamura.dw,'.'...
    )
subplot(3,1,2),...
loglog(...    
tamura(1).tamura.freq_range,tamura(1).tamura.life,'.',...
    alloy.freq,alloy.life,'.'...
    )
subplot(3,1,3),...
loglog(...    
    tamura(1).tamura.freq_range,abs(tamura(1).tamura.g3Iwg2),'.',...
    tamura(1).tamura.freq_range,abs(tamura(1).tamura.g4Iwg2),'.'...
    )




