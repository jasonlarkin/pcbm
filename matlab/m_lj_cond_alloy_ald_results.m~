function ald = m_lj_cond_alloy_ald_results
%VC = m_lj_alloy_nmd_vc_results
%returns the vc thermal conductivity results @ 10K for 4-12x systems
%--------------------------------------------------------------------------

lj = m_lj; constant = m_constant;

ald.m = [1;1.1;1.3;2]; 
ald.prefactor =...
    (3/2)*( (pi/6)^(1/3))*constant.kb*...
    lj.num_density^(2/3);
ald.high_scatter = [...
    ald.prefactor*lj.sound*(lj.sigma/lj.tau)/sqrt(ald.m(1))
    ald.prefactor*lj.sound*(lj.sigma/lj.tau)/sqrt(ald.m(2))
    ald.prefactor*lj.sound*(lj.sigma/lj.tau)/sqrt(ald.m(3))
    ald.prefactor*lj.sound*(lj.sigma/lj.tau)/sqrt(ald.m(4))
    ];

ald.conc =[...
    0
    0.05
    0.15
    0.5
    ];

ald.size =[...
    4
    6
    8
    10
    ];

ald.extrap =[...
    3.2804
    0.4528
    0.2454
    0.2297 
    ];
ald.extrap_diff =[...
    0.0 
    0.5179 
    0.3335
    0.3078
    ];

ald.cond(:,1)=[...
3.75777
3.26345
3.30179
3.27608
];

ald.cond(:,2)=[...
0.374
0.384
0.404
0.4244
];
ald.cond_diff(:,2)=[...
0.417
0.437
0.457
0.4777
];

ald.cond(:,3)=[...
0.194
0.204
0.214
0.2248
];
ald.cond_diff(:,3)=[...
0.223
0.243
0.263
0.2834
];

ald.cond(:,4)=[...
0.178
0.188
0.198
0.2086
];
ald.cond_diff(:,4)=[...
0.193
0.213
0.233
0.2537
];

ald.perfect =[...
	3.3298
	3.1220
	2.8287
	2.3
	];

end
