clear
con = m_constant;

Di(5,1).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perfect3/anneal/b1/','Di.gout');
VOLUME = (5*5.43E-10)^3;
cond(5,1) = (con.kb / VOLUME)*sum(Di(5,1).Di)

Di(5,10).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perfect3/anneal/b10/','Di.gout');
VOLUME = (5*5.43E-10)^3;
cond(5,10) = (con.kb / VOLUME)*sum(Di(5,10).Di)

Di(5,20).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perfect3/anneal/b20/','Di.gout');
VOLUME = (5*5.43E-10)^3;
cond(5,20) = (con.kb / VOLUME)*sum(Di(5,20).Di)

Di(5,100).Di =...
    m_gulp_af_si_readDi(...
    '/home/jason/disorder2/si/amor/normand/perfect3/anneal/b100/','Di.gout');
VOLUME = (5*5.43E-10)^3;
cond(5,100) = (con.kb / VOLUME)*sum(Di(5,100).Di)

loglog(...
    Di(5,1).Di(:,2),Di(5,1).Di(:,3),...
    Di(5,10).Di(:,2),Di(5,10).Di(:,3),...
    Di(5,20).Di(:,2),Di(5,20).Di(:,3),...
    Di(5,100).Di(:,2),Di(5,100).Di(:,3),...
    linspace(min(Di(5,100).Di(:,2)),max(Di(5,100).Di(:,2)),100),...
    1E21*linspace(min(Di(5,100).Di(:,2)),max(Di(5,100).Di(:,2)),100).^(-2),...
    linspace(min(Di(5,100).Di(:,2)),max(Di(5,100).Di(:,2)),100),...
    1E47*linspace(min(Di(5,100).Di(:,2)),max(Di(5,100).Di(:,2)),100).^(-4)...
    )

cond(1000,1)
cond(1000,10)
