clear
constant = m_constant;
size_big = 100000;

gk = m_lj_cond_alloy_gk_results

plot(...
    1./[gk.size;size_big] , 1./[gk.cond(:,2);0.8] , '.',...
    1./[gk.size;size_big] , 1./[gk.cond(:,3);0.5] , '.',...
    1./[gk.size;size_big] , 1./[gk.cond(:,4);0.3] , '.'...
    )
[p R] = m_cond_extrap([gk.size], [gk.cond(:,2)]);
R
1./p(2)
[p R] = m_cond_extrap([gk.size], [gk.cond(:,3)]);
R
1./p(2)
[p R] = m_cond_extrap([gk.size], [gk.cond(:,4)]);
R
1./p(2)
%--------------------------------------------------------------------------
pause
