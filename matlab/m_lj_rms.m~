function urms = m_lj_rms( T , m , freq , eigvec , n , N )  

lj = m_lj; constant = m_constant;

m = m*lj.mass;

freq = freq/lj.tau;
%remove 0-freq
I = find(freq > 0.0001); I1 = find(freq < 0.0001);
freq(I1)
min(freq)

max(freq)

I(1:10)

f = 1./((constant.hbar*freq(I))/(constant.kb*T));

MASS = ones(3*length(m),1); 

MASS(1:3:length(MASS)) = MASS(1:3:length(MASS)).*m;
MASS(2:3:length(MASS)) = MASS(1:3:length(MASS)).*m;
MASS(3:3:length(MASS)) = MASS(1:3:length(MASS)).*m;

size(f./freq(I))

size(eigvec(:,I).*conj(eigvec(:,I)))

size(f'./freq(I)')

max(f'./freq(I)')

size(1./MASS)

MASS(1)

freq(I(9:16))'

f(9:16)'

f(9:16)'./freq(I(9:16))'

eigvec(9:16,I(9:16)).*conj(eigvec(9:16,I(9:16)))

bsxfun(@times,...
    eigvec(9:16,I(9:16)).*conj(eigvec(9:16,I(9:16))),...
    f(9:16)'./(freq(I(9:16))')...
    )

(constant.hbar/(n*N))

sum(...
    bsxfun(...
    @times,1./MASS,...
        sum(...
            bsxfun(@times,...
                eigvec(:,I).*conj(eigvec(:,I)),...
                f'./(freq(I)')...
            ),2)...
        ),1)

max(...
    sum(...
    bsxfun(@times,...
    eigvec(:,I).*conj(eigvec(:,I)),...
    f'./(freq(I)')...
    ),2)...
    )
min(...
    sum(...
    bsxfun(@times,...
    eigvec(:,I).*conj(eigvec(:,I)),...
    f'./(freq(I)')...
    ),2)...
    )
max(...
    bsxfun(...
    @times,1./MASS,...
    sum(...
    bsxfun(@times,...
    eigvec(:,I).*conj(eigvec(:,I)),...
    f'./(freq(I)')...
    ),2)...
    )...
    )
max(...
    bsxfun(...
    @times,1./MASS,...
    sum(...
    bsxfun(@times,...
    eigvec(:,I).*conj(eigvec(:,I)),...
    f'./(freq(I)')...
    ),2)...
    )...
    )
            

urms =...
    (constant.hbar/(n*N))*...
    sum(...
    bsxfun(...
    @times,1./MASS,...
    sum(...
    bsxfun(@times,...
    eigvec(:,I).*conj(eigvec(:,I)),...
    f'./(freq(I)')...
    ),2)...
    ),1);

urms = sqrt(urms);
    
end